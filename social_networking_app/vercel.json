{
  "version": 2,
  "builds": [
    {
      "src": "social_networking-app/social_networking_app/wsgi.py",
      "use": "@vercel/python",
      "config": {"maxLambdaSize": "15mb", "runtime": "python3.9"}
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "social_networking_app/social_networking_app/wsgi.py" }
  ]
}